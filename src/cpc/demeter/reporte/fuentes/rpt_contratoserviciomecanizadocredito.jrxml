	<queryString>
		<![CDATA[SELECT
administracion.tbl_dem_estado_contrato."str_descripcion" AS estadocontrato,
to_char(administracion.tbl_dem_contrato."dtm_fecha", 'dd-mm-yyyy') AS contratodtm_fecha,
to_char(administracion.tbl_dem_contrato."dtm_fechadesde",'dd-mm-yyyy') AS contratofechadesde,
tbl_dem_contrato.int_idtipocontrato AS tipocontrato,
tbl_dem_contrato."int_diasvigencia" AS contratodiasvigencia,
trunc(administracion.tbl_dem_contrato.dbl_monto,2) AS contratomonto,
fu_numero_letras(administracion.tbl_dem_contrato.dbl_monto) AS letrasmonto,
administracion.tbl_dem_contrato."str_serie" AS contratostr_serie,
tbl_dem_clientes."str_nombre" as nombrepagador,
tbl_dem_clientes."str_cedurif"  AS cedurifpagador,
administracion.tbl_dem_contrato."int_nrocontrol" AS contrato_nrocontrol,
tbl_dem_plan_financiamiento."str_descripcion" AS plan_financiamiento_str_descripcion,
tbl_dem_plan_financiamiento."bol_objetoventa" AS plan_financiamiento_bol_objetoventa,
tbl_dem_plan_financiamiento."dtm_fecha" AS plan_financiamiento_dtm_fecha,
tbl_dem_plan_financiamiento."dbl_porcdesc" AS plan_financiamiento_dbl_porcdesc,
fu_numero_letras(administracion.tbl_dem_contrato.dbl_monto*tbl_dem_plan_financiamiento."dbl_porcinic") as letrasinicial,
trunc((administracion.tbl_dem_contrato.dbl_monto*tbl_dem_plan_financiamiento."dbl_porcinic"),2) as inicial,
tbl_dem_plan_financiamiento."dbl_porcinic" AS plan_financiamiento_dbl_porcinic,
fu_numero_letras(administracion.tbl_dem_contrato.dbl_monto-(administracion.tbl_dem_contrato.dbl_monto*tbl_dem_plan_financiamiento."dbl_porcinic")) as letrasculminacion,
trunc((administracion.tbl_dem_contrato.dbl_monto-(administracion.tbl_dem_contrato.dbl_monto*tbl_dem_plan_financiamiento."dbl_porcinic")),2) as culminacion,
tbl_dem_tipocontrato."str_nombre" as tipocontrato_str_nombre,
tbl_dem_tipocontrato."str_clausula" AS dem_tipocontrato_str_clausula,
tbl_dem_control_sede."str_abogado" AS abogado,
tbl_dem_control_sede."str_impreabogado" AS impreabogado,
tbl_dem_control_sede."str_direccionsede" AS direccionsede,
tbl_dem_control_sede."str_coordinadorsede" AS coordinadorsede,
tbl_dem_control_sede."str_sede" AS str_sede,
tbl_dem_control_sede."str_codemp" AS str_codemp,

productor."str_nombre" as nombreproductor,
productor."str_cedurif"  AS cedurifproductor,
sigesp_estados."desest" AS estadocliente,
sigesp_municipio."denmun" AS municipiocliente,
sigesp_parroquia."denpar" AS parroquiacliente,
tbl_dem_direccion."str_infadicional" AS direccioninfadicionalcliente,
tbl_dem_direccion."str_ptoreferencia" AS direccionptoreferenciacliente,
tbl_dem_direccion."str_sector" AS direccionsectorcliente,

tbl_dem_control_sede."str_cedulacoordinadorsede" AS cedulacoordinadorsede,

estadosproduccion."desest" AS estadoproduccion,
municipioproduccion."denmun" AS municipioproduccion,
parroquiaproduccion."denpar" AS parroquiaproduccion,
direccionproduccion."str_infadicional" AS direccionproduccion,
direccionproduccion."str_ptoreferencia" AS direccionptoreferenciaproduccion,
direccionproduccion."str_sector" AS direccionsectorproduccion,
administracion.tbl_dem_contrato.int_diasvigencia AS diasvigencia,
