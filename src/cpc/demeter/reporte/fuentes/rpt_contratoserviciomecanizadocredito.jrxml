FLOOR(date_part('day',administracion.tbl_dem_contrato."dtm_fechadesde" )) as diafirma,
FLOOR (date_part('month',administracion.tbl_dem_contrato."dtm_fechadesde")) as mesfirma,
FLOOR (date_part('year',administracion.tbl_dem_contrato."dtm_fechadesde" )) as añofirma,
FLOOR (date_part('day',administracion.tbl_dem_contrato."dtm_fecha")) as diaculminacion,
FLOOR (date_part('month',administracion.tbl_dem_contrato."dtm_fecha")) as mesculminacion


FROM administracion.tbl_dem_contratomecanizado
LEFT JOIN administracion.tbl_dem_contrato ON administracion.tbl_dem_contratomecanizado.int_idcontrato = administracion.tbl_dem_contrato.seq_idcontrato
LEFT JOIN administracion.tbl_dem_estado_contrato ON tbl_dem_estado_contrato.seq_idestadocontrato = tbl_dem_contrato.int_estado
LEFT JOIN "public"."tbl_dem_clientes" tbl_dem_clientes ON tbl_dem_contrato."int_idpagador" = tbl_dem_clientes."seq_idcliente"
LEFT JOIN "administracion"."tbl_dem_plan_financiamiento" tbl_dem_plan_financiamiento ON tbl_dem_contrato."int_idplanfinanciamiento" = tbl_dem_plan_financiamiento."seq_idplanfinanciamiento"
LEFT JOIN "administracion"."tbl_dem_tipocontrato" tbl_dem_tipocontrato ON tbl_dem_contrato."int_idtipocontrato" = tbl_dem_tipocontrato."seq_idtipocontrato"
LEFT JOIN  public.tbl_dem_solicitud on public.tbl_dem_solicitud.seq_idsolicitud = administracion.tbl_dem_contratomecanizado.int_idsolicitudmecanizado
LEFT JOIN administracion.tbl_dem_control_sede on tbl_dem_control_sede.str_sede=tbl_dem_solicitud.int_idsede
LEFT JOIN "public"."tbl_dem_clientes" productor ON public.tbl_dem_solicitud.int_idcliente= productor."seq_idcliente"
LEFT JOIN public.tbl_dem_direccion tbl_dem_direccion ON public.tbl_dem_solicitud.int_idcliente = tbl_dem_direccion."int_idcliente"
LEFT JOIN "sigesp"."sigesp_parroquia" sigesp_parroquia ON tbl_dem_direccion."str_codpais" = sigesp_parroquia."codpai"
     AND sigesp_parroquia."codmun" = tbl_dem_direccion."str_codmun"
     AND sigesp_parroquia."codpar" = tbl_dem_direccion."str_codpar"
     AND sigesp_parroquia."codest" = tbl_dem_direccion."str_codest"
LEFT JOIN "sigesp"."sigesp_municipio" sigesp_municipio ON sigesp_parroquia."codpai" = sigesp_municipio."codpai"
     AND sigesp_municipio."codmun" = sigesp_parroquia."codmun"
     AND sigesp_municipio."codest" = sigesp_parroquia."codest"
LEFT JOIN "sigesp"."sigesp_municipio" sigesp_municipio_A ON sigesp_parroquia."codpai" = sigesp_municipio_A."codpai"
     AND sigesp_municipio_A."codmun" = sigesp_parroquia."codmun"
     AND sigesp_municipio_A."codest" = sigesp_parroquia."codest"
LEFT JOIN "sigesp"."sigesp_estados" sigesp_estados ON sigesp_municipio_A."codpai" = sigesp_estados."codpai"
     AND sigesp_estados."codest" = sigesp_municipio_A."codest"
     AND sigesp_estados."codpai" = sigesp_municipio."codpai"
     AND sigesp_estados."codest" = sigesp_municipio."codest"

LEFT JOIN public.tbl_dem_direccion direccionproduccion ON administracion.tbl_dem_contratomecanizado.int_idunidadproductiva = direccionproduccion.seq_iddireccion
LEFT JOIN "sigesp"."sigesp_parroquia" parroquiaproduccion ON direccionproduccion."str_codpais" = sigesp_parroquia."codpai"
     AND parroquiaproduccion."codmun" = direccionproduccion."str_codmun"
     AND parroquiaproduccion."codpar" = direccionproduccion."str_codpar"
     AND parroquiaproduccion."codest" = direccionproduccion."str_codest"
LEFT JOIN "sigesp"."sigesp_municipio" municipioproduccion ON parroquiaproduccion."codpai" = municipioproduccion."codpai"
     AND municipioproduccion."codmun" = parroquiaproduccion."codmun"
     AND municipioproduccion."codest" = parroquiaproduccion."codest"
LEFT JOIN "sigesp"."sigesp_municipio" municipioproduccionA ON parroquiaproduccion."codpai" = municipioproduccionA."codpai"
     AND municipioproduccionA."codmun" = parroquiaproduccion."codmun"
     AND municipioproduccionA."codest" = parroquiaproduccion."codest"
LEFT JOIN "sigesp"."sigesp_estados"  estadosproduccion ON municipioproduccionA."codpai" = estadosproduccion."codpai"
     AND estadosproduccion."codest" = municipioproduccionA."codest"
     AND estadosproduccion."codpai" = municipioproduccionA."codpai"
     AND estadosproduccion."codest" = municipioproduccionA."codest"
where tbl_dem_contrato.int_idtipocontrato =1  and tbl_dem_contrato.seq_idcontrato= $P{NumeroContrato}]]>
